{
  "name": "ile-iwe",
  "version": "1.0.0",
  "description": "A unique diary for anyone ( diary that speaks your language )",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node --presets env src/app.js",
    "test": "cross-env NODE_env=test nyc mocha --require babel-core/register src/**/test.js --timeout 8000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devPinheiro/Patako.git"
  },
  "keywords": [
    "Notepad",
    "Multi-language Diary",
    "Multi-Cultural Diary",
    "Diary",
    "Online-Diary"
  ],
  "author": "Pinheiro Samuel",
  "license": "ISC",
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
    "joi": "^14.0.1",
    "mocha": "^5.2.0",
    "mongoose": "^5.3.12",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.7",
    "nyc": "^13.1.0"
  },
  "devDependencies": {}
}
